{
  "name": "gastronomi-api",
  "version": "0.0.1",
  "description": "GASTRONOMI API",
  "main": "app.js",
  "jest": {
    "collectCoverage": true,
    "maxConcurrency": 20,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/services/access-token-service.{js,jsx}",
      "!vendor/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "scripts": {
    "test": "npx jest --forceExit",
    "start": "node server.js",
    "watch": "nodemon app.js"
  },
  "keywords": [
    "api"
  ],
  "dependencies": {
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1233.0",
    "body-parser": "^1.20.1",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql-promise": "^5.0.0",
    "nodemon": "^2.0.22",
    "path": "^0.12.7",
    "pdf-lib": "^1.17.1",
    "serverless-http": "^3.0.3",
    "stream": "^0.0.2",
    "supertest": "^6.3.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "jest": "^29.2.1"
  }
}
